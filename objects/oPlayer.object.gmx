}
*/
if keyboard_check(vk_shift)
{
    if keyboard_check(vk_left)
    {
        pos_check[0] -= speed_walk;
    }
    else if keyboard_check(vk_right)
    {
        pos_check[0] += speed_walk;
    }
    if keyboard_check(vk_up)
    {
        pos_check[1] -= speed_walk;
    }
    else if keyboard_check(vk_down)
    {
        pos_check[1] += speed_walk;
    }
}
else
{
    if keyboard_check_pressed(vk_left)
    {
        pos_check[0] -= speed_run;
    }
    else if keyboard_check_pressed(vk_right)
    {
        pos_check[0] += speed_run;
    }
    if keyboard_check_pressed(vk_up)
    {
        pos_check[1] -= speed_run;
    }
    else if keyboard_check_pressed(vk_down)
    {
        pos_check[1] += speed_run;
    }
}

var cell_next = oGame.map[round(pos_check[0]), round(pos_check[1])]

if cell_next != oGame.rock
{
    x = pos_check[0];
    y = pos_check[1];
    if x &lt; 0
    {
        oGame.generate = true;
        x = 63;
    }
    else if x &gt; 63
    {
        oGame.generate = true;
        x = 0;
    }
    if y &lt; 0
    {
        oGame.generate = true;
        y = 47;
    }
    else if y &gt; 47
    {
        oGame.generate = true;
        y = 0;
    }
}
else
{
    cell_next = oGame.space;
    if !audio_is_playing(sndBump)
        audio_play_sound(sndBump, 1, false);
}
oGame.map_display[round(x), round(y)] = global.player;
